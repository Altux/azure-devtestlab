{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "newVMName": {
      "type": "string",
      "defaultValue": "TEST2"
    },
    "labName": {
      "type": "string",
      "defaultValue": "TEST-CLasse1"
    },
    "size": {
      "type": "string",
      "defaultValue": "Standard_DS11_v2"
    },
    "userName": {
      "type": "string",
      "defaultValue": "Administrateur"
    },
    "password": {
      "type": "securestring"
    },
    "Install_Chocolatey_Packages.packages": {
      "type": "string",
      "defaultValue": "googledrive"
    },
    "Change_windows_RDP_port.Port": {
      "type": "string",
      "defaultValue": "443"
    },
    "Install_Chrome_Extention.ID": {
      "type": "string",
      "defaultValue": "gbchcmhmhahfdphkhkmpfmihenigjmpp"
    },
    "New_local_Administrator.Name": {
      "type": "string",
      "defaultValue": "Administrateur2"
    },
    "New_local_Administrator.Password": {
      "type": "string",
      "defaultValue": "SuperPassword!"
    },
    "Post_Deployment_Script.URL": {
      "type": "string",
      "defaultValue": "https://raw.githubusercontent.com/Altux/azure-devtestlab/master/Config%20File/ESCP/ESCP-PostDeployment.ps1"
    },
    "Windows_Optional_Feature.FeatureName": {
      "type": "string",
      "defaultValue": "Internet-Explorer-Optional-amd64"
    },
    "Windows_Optional_Feature.Action": {
      "type": "string",
      "defaultValue": "Disable"
    }
  },
  "variables": {
    "labSubnetName": "[concat(variables('labVirtualNetworkName'), 'Subnet')]",
    "labVirtualNetworkId": "[resourceId('Microsoft.DevTestLab/labs/virtualnetworks', parameters('labName'), variables('labVirtualNetworkName'))]",
    "labVirtualNetworkName": "[concat('Dtl', parameters('labName'))]",
    "vmId": "[resourceId ('Microsoft.DevTestLab/labs/virtualmachines', parameters('labName'), parameters('newVMName'))]",
    "vmName": "[concat(parameters('labName'), '/', parameters('newVMName'))]"
  },
  "resources": [
    {
      "apiVersion": "2016-05-15",
      "type": "Microsoft.DevTestLab/labs/virtualmachines",
      "name": "[variables('vmName')]",
      "location": "[resourceGroup().location]",
      "properties": {
        "labVirtualNetworkId": "[variables('labVirtualNetworkId')]",
        "notes": "Windows Server 2016 Datacenter",
        "galleryImageReference": {
          "offer": "WindowsServer",
          "publisher": "MicrosoftWindowsServer",
          "sku": "2016-Datacenter",
          "osType": "Windows",
          "version": "latest"
        },
        "size": "[parameters('size')]",
        "userName": "[parameters('userName')]",
        "password": "[parameters('password')]",
        "isAuthenticationWithSshKey": false,
        "artifacts": [
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'public repo', 'windows-chocolatey')]",
            "parameters": [
              {
                "name": "packages",
                "value": "[parameters('Install_Chocolatey_Packages.packages')]"
              }
            ]
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'repo-jnur3wqm66744', 'windows-androidstudio')]"
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'repo-jnur3wqm66744', 'windows-AppleKeyboardLayout')]"
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'repo-jnur3wqm66744', 'windows-setwindowsrdpport')]",
            "parameters": [
              {
                "name": "Port",
                "value": "[parameters('Change_windows_RDP_port.Port')]"
              }
            ]
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'repo-jnur3wqm66744', 'windows-ieenforcement')]"
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'repo-jnur3wqm66744', 'windows-donotopenservermanageratlogon')]"
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'repo-jnur3wqm66744', 'windows-chromeextention')]",
            "parameters": [
              {
                "name": "ID",
                "value": "[parameters('Install_Chrome_Extention.ID')]"
              }
            ]
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'repo-jnur3wqm66744', 'windows-newlocaladmin')]",
            "parameters": [
              {
                "name": "Name",
                "value": "[parameters('New_local_Administrator.Name')]"
              },
              {
                "name": "Password",
                "value": "[parameters('New_local_Administrator.Password')]"
              }
            ]
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'repo-jnur3wqm66744', 'windows-postdeployment')]",
            "parameters": [
              {
                "name": "URL",
                "value": "[parameters('Post_Deployment_Script.URL')]"
              }
            ]
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'repo-jnur3wqm66744', '-windows-shutdown-popup')]"
          },
          {
            "artifactId": "[resourceId('Microsoft.DevTestLab/labs/artifactSources/artifacts', parameters('labName'), 'repo-jnur3wqm66744', 'windows-optionalfeature')]",
            "parameters": [
              {
                "name": "FeatureName",
                "value": "[parameters('Windows_Optional_Feature.FeatureName')]"
              },
              {
                "name": "Action",
                "value": "[parameters('Windows_Optional_Feature.Action')]"
              }
            ]
          }
        ],
        "labSubnetName": "[variables('labSubnetName')]",
        "disallowPublicIpAddress": false,
        "storageType": "Standard",
        "allowClaim": false
      }
    }
  ],
  "outputs": {
    "labVMId": {
      "type": "string",
      "value": "[variables('vmId')]"
    }
  }
}